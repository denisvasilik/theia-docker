{
    "editor.renderWhitespace": "all",
    "terminal.integrated.fontFamily": "Menlo for Powerline",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true
      },
    "editor.rulers": [
        80,
        100
    ],
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 15,
    "workbench.iconTheme": "theia-file-icons",
    "python.pythonPath": "/usr/bin/python"
}